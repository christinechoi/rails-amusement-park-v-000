<h1>Sessions#new</h1>


<h3> Sign In: </h3>


<%= form_for :user, :url => signin_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.submit "Sign In" %>
<% end %>

